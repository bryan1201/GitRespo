@model IEnumerable<B2BService.Models.DBSession>
<table class="table table-bordered table-striped table-condensed">
    <tr>
        <th>@Html.DisplayNameFor(model => model.Name)</th>
        <th>@Html.DisplayNameFor(model => model.NameValue)</th>
        <th>@Html.DisplayNameFor(model => model.Percent)</th>
    </tr>
    @foreach (var Item in Model)
    {
        <tr>
            <td>@Html.DisplayFor(modelItem => Item.Name)</td>
            <td>@Html.DisplayFor(modelItem => Item.NameValue)</td>
            <td>@Html.DisplayFor(modelItem => Item.Percent) %</td>
        </tr>
    }
</table>