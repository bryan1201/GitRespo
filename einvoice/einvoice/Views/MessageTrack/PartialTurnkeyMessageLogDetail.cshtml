@model IEnumerable<einvoice.Models.TURNKEY_MESSAGE_LOG_DETAIL>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SEQNO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SUBSEQNO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PROCESS_DTS)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TASK)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.STATUS)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FILENAME)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.SEQNO)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SUBSEQNO)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PROCESS_DTS)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TASK)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.STATUS)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FILENAME)
        </td>
        <td>
            @Html.ActionLink("RawData", "RawData", new { filename=item.FILENAME })
        </td>
    </tr>
}

</table>
