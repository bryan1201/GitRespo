@model Meetings.Models.MeetingDateActivate

@{
    ViewBag.Title = "Edit MeetingDateActivate";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@ViewBag.Title</h2>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)

    <fieldset>
        <legend>Meeting</legend>

        @Html.HiddenFor(model => model.meetingDateId)
        @Html.HiddenFor(model => model.ActivateId)
        <div class="raw">
            <div class="col-md-12">
                <table class="table table-bordered table-striped table-condensed">
                    <tr>
                        <th class="editor-label">@Html.LabelFor(model => model.ActivateId)</th>
                        <td class="editor-field">@Html.DisplayFor(model => model.ActivateId)</td>
                    </tr>
                    <tr>
                        <th class="editor-label">@Html.LabelFor(model => model.ActivateDesc)</th>
                        <td class="editor-field">@Html.EditorFor(model => model.ActivateDesc)@Html.ValidationMessageFor(model => model.ActivateDesc)</td>
                    </tr>
                    <tr>
                        <th class="editor-label">@Html.LabelFor(model => model.ActivateDate)</th>
                        <td class="editor-field">@Html.EditorFor(model => model.ActivateDate)@Html.ValidationMessageFor(model => model.ActivateDate)</td>
                    </tr>
                    <tr>
                        <th class="editor-label">@Html.LabelFor(model => model.Sex)</th>
                        <td class="editor-field">@Html.EditorFor(model => model.Sex)@Html.ValidationMessageFor(model => model.Sex)</td>
                    </tr>
                    <tr>
                        <th class="editor-label">@Html.LabelFor(model => model.SDT)</th>
                        <td class="editor-field">@Html.EditorFor(model => model.SDT)@Html.ValidationMessageFor(model => model.SDT)</td>
                    </tr>
                    <tr>
                        <th class="editor-label">@Html.LabelFor(model => model.EDT)</th>
                        <td class="editor-field">@Html.EditorFor(model => model.EDT)@Html.ValidationMessageFor(model => model.EDT)</td>
                    </tr>
                </table>
            </div>
        </div>
        <p>
            <input type="submit" value="Save" />
        </p>
    </fieldset>
}

<div>
    @Html.ActionLink("Back to List", "Index", new {id=Model.meetingDateId })
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
