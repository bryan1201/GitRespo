                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="True">
                    <CommandText>SELECT DISTINCT CustomerID,SiteID,ProjectID,ProjectName,PhaseID,PhaseName,PhaseInputQty,
			CONVERT(NVARCHAR(10),StartDate,111) as StartDate,
			CONVERT(NVARCHAR(10),EndDate,111) as EndDate,
			[dbo].[fn_GetMaterialYield](@Customer,@ProjectID,f.MaterialType,'PreTestYield','CPU') as PreTestYield,
			[dbo].[fn_GetMaterialYield](@Customer,@ProjectID,f.MaterialType,'RunInYield','CPU')  as RunInYield,
			[dbo].[fn_GetMaterialYield](@Customer,@ProjectID,f.MaterialType,'FPY','CPU') as FPY,
			MaterialType,
			[dbo].[fn_GetTotalIssueQty](@Customer,@Site,@PhaseID,@ProjectID,MaterialType,'CPU') as TotalIssueQty,
			[dbo].[fn_GetStatusOpenQty](@Customer,@Site,v.PhaseID,v.ProjectID,MaterialType,'CPU') as OpenQty,
			[dbo].[fn_GetStatusCloseQty](@Customer,@Site,@PhaseID,@ProjectID,MaterialType,'CPU') as CloseQty,
			[dbo].[fn_GetStatusCloseRate](@Customer,@Site,@PhaseID,@ProjectID,MaterialType,'CPU') as CloseRate,
			'ListCPUDetail.aspx' as URL
			FROM [v_ProjectPhase] v INNER JOIN f_common f ON
			v.CustomerID=f.Customer AND v.SiteID=f.Site 
			AND v.ProjectID=f.Project AND  v.PhaseID=f.Phase
			WHERE CustomerID=@Customer AND SiteID=@Site
			AND dbo.fn_GetProjectMaterialType(f.Project, f.MaterialType) = 'CPU'
			AND ProjectID=@ProjectID AND PhaseID=@PhaseID;</CommandText>
