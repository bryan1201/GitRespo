@model IEnumerable<Meetings.Models.vMeetingDateMemberLog>
<table class="table table-bordered table-striped table-condensed">
    <tr>
        <th style="width:20%">
            @Html.DisplayNameFor(model => model.FirstOrDefault().MeetingName)
        </th>
        <th style="width:15%">
            @Html.DisplayNameFor(model => model.FirstOrDefault().Description)
        </th>
        <th style="width:20%">
            @Html.DisplayNameFor(model => model.FirstOrDefault().UserCode)
        </th>
        <th style="width:20%">
            @Html.DisplayNameFor(model => model.FirstOrDefault().UserName)
        </th>
        <th style="width:25%">
            @Html.DisplayNameFor(model => model.FirstOrDefault().LoginTime)
        </th>
    </tr>

    @foreach (var item in Model)
    {
        if (item != null)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelitem => item.MeetingName)
                </td>
                <td>
                    @Html.DisplayFor(modelitem => item.Description)
                </td>
                <td>
                    @Html.DisplayFor(modelitem => item.UserCode)
                </td>
                <td>
                    @Html.DisplayFor(modelitem => item.UserName)
                </td>
                <td>
                    @Html.DisplayFor(modelitem => item.LoginTime)
                </td>
            </tr>
        }
    }
</table>
