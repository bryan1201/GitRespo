@model Meetings.Models.MeetingDateMember
@{
    ViewBag.Title = "Create Meeting";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@ViewBag.Title</h2>
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)
    if (ViewBag.Message != null)
    {
        <div class="alert alert-warning">
            <strong>Message:</strong> @ViewBag.Message
        </div>
    }
    <fieldset>
        <legend>MeetingDateMember</legend>
        @Html.HiddenFor(model => model.meetingDateId)
        @Html.HiddenFor(model => model.uniqueId)
        @Html.HiddenFor(model => model.UserProfileId)
        <table class="table table-bordered table-striped table-condensed">
            <tr>
                <th class="editor-label">@Html.LabelFor(model => model.uniqueId)</th>
                <td class="editor-field">@Html.ValueFor(model => model.uniqueId)</td>
            </tr>
            <tr>
                <th class="editor-label">@Html.LabelFor(model => model.meetingDateId)</th>
                <td class="editor-field">@Html.ValueFor(model => model.meetingDateId)</td>
            </tr>
            <tr>
                <th class="editor-label">@Html.LabelFor(model => model.UserProfileId)</th>
                <td class="editor-field">@Html.ValueFor(model => model.UserProfileId)</td>
            </tr>

            <tr>
                <th class="editor-label">@Html.LabelFor(model => model.UserCode)</th>
                <td class="editor-field">@Html.EditorFor(model => model.UserCode)@Html.ValidationMessageFor(model => model.UserCode)</td>
            </tr>
        </table>
        <p>
            <input type="submit" value="Create" />
        </p>
    </fieldset>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}

