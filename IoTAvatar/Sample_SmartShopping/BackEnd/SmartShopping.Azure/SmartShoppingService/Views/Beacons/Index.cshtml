@model IEnumerable<SmartShoppingDemoService.Models.Beacon>

@{
    ViewBag.Title = "Beacons Table";
}

<h2>@ViewBag.Title</h2>

<p>Note: When data in Beacons table is changed, required to restart http://smartshoppingdemoprocess.azurewebsites.net/ to make the changes take effect for supporting multiple instances.</p>
<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.BeaconId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ProductId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StoreId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.InFilter)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OutFilter)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Xaxis)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Yaxis)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Longitude)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LatuLatitude)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.BeaconId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ProductId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StoreId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.InFilter)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OutFilter)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Xaxis)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Yaxis)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Longitude)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LatuLatitude)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
