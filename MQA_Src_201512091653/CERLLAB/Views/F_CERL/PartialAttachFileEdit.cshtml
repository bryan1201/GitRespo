@model IEnumerable<CERLLAB.Models.vattachFileMaxVer>
<table id="browser" class="jtable" style="table-layout: fixed; font-size: 1em; border: 0px;">
    @foreach (var item in Model)
    {
        <tr>
            <td style="width:20px;">
                @Html.ActionLink(" ", "DeleteFile", "F_CERL", new { fileId = item.fileId, record = false }, new { style = "background: url('" + Url.Content("~/Images/Delete.gif") + "') no-repeat top left;background-size: 1.2em auto;height:1.2em;width:auto;display:block;text-indent:-9999px;" })
            </td>
            <td style="width:auto;">
                @Html.ActionLink(item.fileName, "GetFilePath", "f_cerl", new { fileId = item.fileId, record = false }, null)
            </td>
        </tr>
    }
</table>
