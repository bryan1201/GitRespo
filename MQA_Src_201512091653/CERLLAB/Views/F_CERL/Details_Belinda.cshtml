<script type="text/javascript">
    $(document).ready(function () {

        // Hide all the submenu ul/li s
        $("#cerlmenu ul").show();

        $("#cerlmenu li ul").hide();

        // Hook up mouse over events
        $("#cerlmenu li").hover(
            function () {
                var sibling = $(this).find("a").next();
                sibling.show();
            },
            function () {
                var sibling = $(this).find("a").next();
                sibling.hide();
            });
    });
</script>
