                            <th class="display-label" width="15%">
                                @Html.DisplayNameFor(model => model.ProcessStep)
                            </th>
                            <td class="display-field" width="35%">
                                @Html.DisplayFor(model => model.ProcessStepName)
                            </td>
                            <th class="display-label" width="15%">
