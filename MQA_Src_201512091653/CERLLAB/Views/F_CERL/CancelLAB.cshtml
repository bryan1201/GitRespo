                        <tr>
                            <th class="display-label" width="15%">
                                @Html.DisplayNameFor(model => model.TestPurpose)
                            </th>
                            <td class="display-field" width="35%">
                                @Html.DisplayFor(model => model.TestPurposeName)
                            </td>
                            <th class="display-label" width="15%">
                                @Html.DisplayNameFor(model => model.ProductStage)
