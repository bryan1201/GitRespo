                    <table class="jtable" style="table-layout: fixed;">
                        <tr>
                            <th class="display-label" width="15%">
                                User Files
                            </th>
                            <td class="display-field" width="85%">
                                @if (ViewData["attFile10"] != null)
                                {
                                    @Html.Partial(PartialAttachFile, ViewData["attFile10"])
                                }
                            </td>
                        </tr>
                    </table>
                </div>
