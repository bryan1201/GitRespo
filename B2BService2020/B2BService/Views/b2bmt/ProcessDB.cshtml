@model IEnumerable<B2BService.Models.vProcessDB>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    string Id = ViewBag.Id;
}
<div>@Html.DisplayNameFor(model => model.FirstOrDefault().MSGID)</div>
<div><h4>@Html.ValueFor(model => model.FirstOrDefault().MSGID)</h4></div>

<table class="table table-bordered table-striped table-condensed" style="margin-left:20%; margin-right:20%; width:60%">
    <tr>
        <th style="width: 40%;font-size:0.7em; word-wrap: break-word;">
            @Html.DisplayNameFor(model => model.ProcessStep)
        </th>
        <th style="width: 20%;font-size:0.7em; word-wrap: break-word;">
            @Html.DisplayNameFor(model => model.ProcessStatus)
        </th>
        <th style="width:40%;font-size:0.7em; word-wrap: break-word;">
            @Html.DisplayNameFor(model => model.DATETIME)
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td style="font-size:0.9em; word-wrap: break-word;">
                <div>@Html.DisplayFor(modelItem => item.ProcessStepRemark)</div>
            </td>
            <td style="font-size:0.9em; word-wrap: break-word;">
                <div>@Html.DisplayFor(modelItem => item.ProcessStatusRemark)</div>
            </td>
            <td style="font-size:0.9em; word-wrap: break-word;">
               @Html.DisplayFor(modelItem => item.DATETIME)
            </td>
        </tr>
    }
</table>

